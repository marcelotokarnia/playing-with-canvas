{
  "name": "playing-with-canvas",
  "version": "1.0.0",
  "description": "Playing with canvas and geometry",
  "main": "index.js",
  "devDependencies": {
    "concurrently": "3.5.1",
    "eslint": "^4.18.2",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "react-scripts": "1.1.1"
  },
  "dependencies": {
    "autoprefixer-stylus": "0.14.0",
    "classnames": "^2.2.5",
    "prop-types": "^15.6.0",
    "ramda": "^0.25.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "react-transition-group": "^2.2.1",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "serve": "^10.1.2",
    "stylus": "0.54.5"
  },
  "scripts": {
    "dev": "react-scripts start",
    "start": "serve --single ./build",
    "watch": "concurrently --names \"webpack, stylus\" --prefix name \"npm run start\" \"npm run styles:watch\"",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "styles": "stylus -u autoprefixer-stylus ./src/css/style.styl -o ./src/css/style.css",
    "styles:watch": "stylus -u autoprefixer-stylus -w ./src/css/style.styl -o ./src/css/style.css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/marcelotokarnia/playing-with-canvas.git"
  },
  "keywords": [
    "canvas",
    "javascript"
  ],
  "author": "Marcelo Tokarnia <marcelo.tokarnia@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/marcelotokarnia/playing-with-canvas/issues"
  }
}
